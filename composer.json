{
    "name": "mjpr/cookiebot",
    "type": "typo3-cms-extension",
    "description": "Implement cookiebot in TYPO3",
    "authors": [
        {
            "name": "Matthias Pruy",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5 || ^10.1"
    },
    "autoload": {
        "psr-4": {
            "MJPR\\Cookiebot\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MJPR\\Cookiebot\\Tests\\": "Tests"
        }
    },
    "replace": {
        "mjpr/cookiebot": "self.version",
        "typo3-ter/cookiebot": "self.version"
    }
}